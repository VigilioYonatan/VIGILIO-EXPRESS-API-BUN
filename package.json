{
  "name": "@vigilio/express-api",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "create web apps with @vigilio/express now",
  "scripts": {
    "dev": "bun --watch src/main.ts",
    "build": "rimraf dist && tsc  --project tsconfig.build.json ",
    "start": "NODE_ENV=production bun dist/main.js",
    "test": "vitest",
    "biome:format": "biome format  ./src --write",
    "biome:check": "biome check ./src"
  },
  "keywords": [
    "@vigilio/express",
    "@vigilio/express-api",
    "bun"
  ],
  "author": "Vigilio",
  "license": "ISC",
  "dependencies": {
    "@decorators/di": "^2.0.0",
    "@decorators/express": "^2.9.4",
    "@vigilio/express-core": "^1.7.18",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "formidable": "^3.5.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "sequelize": "^6.35.1",
    "sequelize-typescript": "^2.1.6",
    "sharp": "0.32.6",
    "valibot": "^0.20.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@faker-js/faker": "^8.3.1",
    "@types/bun": "^1.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/formidable": "^3.4.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^2.0.16",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.2.2"
  }
}
